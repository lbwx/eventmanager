{{form_start(form, {'attr': {'id': 'manager-event-form'}})}}
	<div class="form-group form-row">
		{{form_label(form.name) }}
		{{form_widget(form.name, {'attr': {'class': 'form-control'}})}}
	</div>
	<div class="form-group form-row">
		{{form_label(form.maxGuests)}}
		{{form_widget(form.maxGuests, {'attr': {'class': 'form-control'}})}}
	</div>
	<div class="form-group form-row">
		{{form_label(form.start)}}
		{{form_widget(form.start, {'attr': {'class': 'form-control'}})}}
	</div>
	<div class="form-group form-row">
		{{form_label(form.end)}}
		{{form_widget(form.end, {'attr': {'class': 'form-control'}})}}
	</div>
	<div class="form-group form-row">
		{{form_label(form.place)}}
		{{form_widget(form.place, {'attr': {'class': 'form-control'}})}}
	</div>
	{{form_row(form.submit, {'label': submitLabel})}}
{{form_end(form)}}